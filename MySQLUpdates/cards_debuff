UPDATE `eternal_cards` SET `debuffarmor` = 0 WHERE `cardtext` NOT LIKE '%-_[Armor]%';
UPDATE `eternal_cards` SET `debuffstrength` = 0 WHERE `cardtext` NOT LIKE '%-_[Strength]%';
UPDATE `eternal_cards` SET `debuffhealth` = 0 WHERE `cardtext` NOT LIKE '%-_[Health]%';
UPDATE `eternal_cards` SET `debuffunit` = 0 WHERE `cardtext` NOT LIKE '%-_/-%';
UPDATE `eternal_cards` SET `debuffarmor` = 1 WHERE `cardtext` LIKE '%-_[Armor]%';
UPDATE `eternal_cards` SET `debuffstrength` = 1 WHERE `cardtext` LIKE '%-_[Strength]%';
UPDATE `eternal_cards` SET `debuffhealth` = 1 WHERE `cardtext` LIKE '%-_[Health]%';
UPDATE `eternal_cards` SET `debuffunit` = 1 WHERE `cardtext` LIKE '%-_/-%';
UPDATE `eternal_cards` SET `debuff` = 1 WHERE( 
		`debuffarmor` = 1 OR 
		`debuffstrength` = 1 OR 
		`debuffhealth` = 1 OR 
		`debuffunit` = 1);
UPDATE `eternal_cards` SET `debuff` = 0 WHERE( 
		`debuffarmor` = 0 AND  
		`debuffstrength` = 0 AND  
		`debuffhealth` = 0 AND  
		`debuffunit` = 0);
