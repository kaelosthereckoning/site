	(`cardtext` LIKE '%deal damage%equal to%' OR
	`cardtext` LIKE '%deal%[Strength] in damage%' OR
	`cardtext` LIKE '%deal%damage% for each%' OR
	`cardtext` LIKE '%deal that much damage%' OR
	`cardtext` LIKE '%increase the damage dealt by%' OR
	`cardtext` LIKE '%deal%damage%increase this ability by%'),1,0);
