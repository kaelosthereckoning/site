	`cardtext` LIKE '%if you have [Primal][Primal] %' OR
	`cardtext` LIKE '%[Primal][Primal]: %' OR
	`cardtext` LIKE '%if you have [Shadow][Shadow] %' OR
	`cardtext` LIKE '%[Shadow][Shadow]: %')
	AND
	(`cardtext` NOT LIKE '%[Fire][Fire][Fire]%' AND
	`cardtext` NOT LIKE '%[Time][Time][Time]%' AND
	`cardtext` NOT LIKE '%[Justice][Justice][Justice]%' AND
	`cardtext` NOT LIKE '%[Primal][Primal][Primal]%' AND
	`cardtext` NOT LIKE '%[Shadow][Shadow][Shadow]%'));
	
