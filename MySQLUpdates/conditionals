UPDATE `eternal_cards` SET `mono1` = 1 WHERE (
	(`cardtext` LIKE '%if you have [Fire] %' OR
	`cardtext` LIKE '%[Fire]: %' OR
	`cardtext` LIKE '%if you have [Time] %' OR
	`cardtext` LIKE '%[Time]: %' OR
	`cardtext` LIKE '%if you have [Justice] %' OR
	`cardtext` LIKE '%[Justice]: %' OR
	`cardtext` LIKE '%if you have [Primal] %' OR
	`cardtext` LIKE '%[Primal]: %' OR
	`cardtext` LIKE '%if you have [Shadow] %' OR
	`cardtext` LIKE '%[Shadow]: %')
	AND
	(`cardtext` NOT LIKE '%[Fire][Fire]%' AND
	`cardtext` NOT LIKE '%[Time][Time]%' AND
	`cardtext` NOT LIKE '%[Justice][Justice]%' AND
	`cardtext` NOT LIKE '%[Primal][Primal]%' AND
	`cardtext` NOT LIKE '%[Shadow][Shadow]%'));
	
