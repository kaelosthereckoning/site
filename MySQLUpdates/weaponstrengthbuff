UPDATE `eternal_cards` SET `WSBvariable` = IF(
	((`cardtype` = 'Weapon' OR `cardtype` = 'Relic Weapon') AND
		(`cardtext` LIKE '%+1/+1 for each%' OR
		`cardtext` LIKE '%+1[Strength] for each%')),1,0);
