UPDATE `eternal_cards` SET `wstrengthbuff` = 1 WHERE (
	((`cardtype` = 'Weapon' OR `cardtype` = 'Relic Weapon') AND
		(`cardtext` LIKE '%Spark: +1/+1%' OR
		`cardtext` LIKE '%+1/+1 this turn%' OR
		`cardtext` LIKE '%+1/+1 for each%')
	) OR
	`cardtext` LIKE '%Warcry affects each%weapon%' OR
	`cardtext` LIKE '%weapon in your deck gets +1/+1%' OR
	`cardtext` LIKE '%weapon of your deck gets +1/+1%' OR
	`cardtext` LIKE '%give a unit or weapon in your hand +1/+1%' OR
	`cardtext` LIKE '%draw a%weapon%it gets +1[Strength]%' OR
	`cardtext` LIKE '%draw a%weapon%it gets +1/+1%' OR
	`cardtext` LIKE '%draw a%weapon%give them +1[Strength]%'
	);
	
