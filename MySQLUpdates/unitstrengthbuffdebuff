UPDATE `eternal_cards` SET `ustrengthbuff` = 0;

UPDATE `eternal_cards` SET `ustrengthbuff` = 1 WHERE (
	(`cardtype` = 'Unit' AND
		(`cardtext` LIKE '%get %+1[Strength]%' OR
		`cardtext` LIKE '%gets %+1[Strength]%' OR
		`cardtext` LIKE '%gets %+1/+%' OR
		`cardtext` LIKE '%get %+1/+%' OR
		`cardtext` LIKE '%has +1/+%' OR
		`cardtext` LIKE '%has +1[Strength]%' OR
		`cardtext` LIKE '%have +1/+%' OR
		`cardtext` LIKE '%have +1[Strength]%' OR
		`cardtext` LIKE '%+1[Strength] while%' OR
		`cardtext` LIKE '%+1/+1 while%' OR
		`cardtext` LIKE '%+1[Strength] and % while%' OR
		`cardtext` LIKE '%+1/+% and % while%' OR
		`cardtext` LIKE '%+1/+% for each%' OR
		`cardtext` LIKE '%+1[Strength] for each%' OR
		`cardtext` LIKE '%give%+1/+1%' OR
		`cardtext` LIKE '%give%+1[Strength]%' OR
		`cardtext` LIKE '%Ally: +1/+%' OR
		`cardtext` LIKE '%Spark: +1/+%'
		))
	OR
		((`cardtype` = 'Unit' OR `cardtype` = 'Weapon' OR `cardtype` = 'Relic Weapon') AND
		(`cardtext` LIKE '%Warcry%'))
	OR
		`cardtext` LIKE '%give a unit +1[Strength]%' OR
		`cardtext` LIKE '%give a unit +1/+%' OR
		`cardtext` LIKE '%give another unit +1[Strength]%' OR
		`cardtext` LIKE '%give another unit +1/+%' OR
		`cardtext` LIKE '%units get +1/+%' OR
		`cardtext` LIKE '%units get +1[Strength]%' OR
		`cardtext` LIKE '%unit each get +1[Strength]%' OR
		`cardtext` LIKE '%unit gets +1[Strength]%' OR
		`cardtext` LIKE '%unit gets +1/+%' OR
		`cardtext` LIKE '%units have +1[Strength]%' OR
		`cardtext` LIKE '%units have +1/+%' OR
		`cardtext` LIKE '%give yourself or a unit +1[Strength]%' OR
		`cardtext` LIKE '%give a unit or weapon in your hand +1/+%' OR
		`cardtext` LIKE '%each unit in your hand with%gets +1/+%' OR
		`cardtext` LIKE '%when%with%unit%it gets +1/+%' OR
		`cardtext` LIKE '%each Gunslinger%gets +1/+%' OR
		`cardtext` LIKE '%each Valkyrie%gets +1/+%'
	);

