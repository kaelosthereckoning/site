UPDATE `eternal_cards` SET `ustrengthdebuff` = 0;
	
UPDATE `eternal_cards` SET `ustrengthdebuff` = -1 WHERE (
	(`cardtype` = 'Unit' AND
		(`cardtext` LIKE '%give%-1/-%')
	) OR
	`cardtext` LIKE '%give%unit%-1[Strength]%' OR
	`cardtext` LIKE '%give%unit%-1/-%' OR
	`cardtext` LIKE '%units get -1[Strength]%' OR
	`cardtext` LIKE '%units get -1/-%' OR
	`cardtext` LIKE '%unit%gets -1[Strength]%' OR
	`cardtext` LIKE '%unit%gets -1/-%' OR
	`cardtext` LIKE '%unit has -1[Strength]%' OR
	`cardtext` LIKE '%unit has -1/-%' OR
	`cardtext` LIKE '%play a -1[Strength]%' OR
	`cardtext` LIKE '%play a -1/-%' OR
	`cardtext` LIKE '%-1/-1%');
	
