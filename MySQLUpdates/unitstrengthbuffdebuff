UPDATE `eternal_cards` SET `USBvariable` = IF(
	((`cardtype` = 'Unit' AND (
		`cardtext` LIKE '%double%[Strength]%' OR
		`cardtext` LIKE '%has%for each%' OR
		`cardtext` LIKE '%gets%for each%' OR
		`cardtext` LIKE '%gets that much [Strength]/[Health]%' OR
		`cardtext` LIKE '%gets that much [Strength]%' OR
		`cardtext` LIKE '%get that much [Strength]/[Health]%' OR
		`cardtext` LIKE '%get that much [Strength]%' OR
		`cardtext` LIKE '%give%that much [Strength] and [Health]%' OR
		`cardtext` LIKE '%give%that much [Strength]%' OR
		`cardtext` LIKE '%give one of your other units Crownwatch Cavalry% [Strength]/[Health]%' OR
		`cardtext` LIKE '%gets the Student% [Strength]/[Health]%' OR
		`cardtext` LIKE '%double its [Strength]/[Health]%' OR
		`cardtext` LIKE '%double the [Strength]/[Health]%' OR
		`cardtext` LIKE '%double%[Strength]%')) OR
		`cardtext` LIKE '%swap%[Strength] and [Health]%'),1,0);
	
UPDATE `eternal_cards` SET `USDvariable` = IF(
	(`cardtext` LIKE '%give%units an additional -%[Strength]%' OR
	`cardtext` LIKE '%give%units an additional -%/-%' OR
	`cardtext` LIKE '%give%unit -%[Strength] equal to%' OR
	`cardtext` LIKE '%give%unit -%/-% equal to%' OR
	`cardtext` LIKE '%each unit% gets -%[Strength] for each%' OR
	`cardtext` LIKE '%each unit% gets -%/-% for each%' OR
	`cardtext` LIKE '%swap%[Strength] and [Health]%'),1,0);
